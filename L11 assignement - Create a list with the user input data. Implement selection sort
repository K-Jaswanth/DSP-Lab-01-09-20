import sys 
lst = [] 
n = int(input("Enter number of elements : "))

for i in range(0, n): 
	ele = int(input()) 

	lst.append(ele)


for i in range(len(lst)): 
	
	min_idx = i 
	for j in range(i+1, len(lst)): 
		if lst[min_idx] > lst[j]: 
			min_idx = j 
			
		 
	lst[i], lst[min_idx] = lst[min_idx], lst[i] 

print ("Sorted array") 
for i in range(len(lst)): 
	print("%d" %lst[i])
